#include <stdio.h>

int main() {
  // Int array with 5 elements
  int blue_balls[5];
  // Getting size by dividing size of array (5*4) to size of array's element (4)
  int size = sizeof(blue_balls) / sizeof(blue_balls[0]);
  // Printing result
  printf("В коробке blue_balls помещается %d шариков.\n", size);
  return 0;
}
